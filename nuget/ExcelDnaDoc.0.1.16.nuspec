<?xml version="1.0" encoding="utf-8"?>
<package xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <metadata xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">    
    <id>ExcelDnaDoc</id>
    <version>0.1.16</version>
    <authors>David Carlson</authors>
    <owners>David Carlson</owners>
    <summary>command-line utility to create a compiled HTML Help Workshop file (.chm) for ExcelDna</summary>
    <licenseUrl>https://github.com/mndrake/ExcelDnaDoc/blob/master/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/mndrake/ExcelDnaDoc</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description> Command-line utility to create a compiled HTML Help Workshop file (.chm) for ExcelDna.  To build compiled help file (.chm) the HTML Help Workshop must be installed.  For examples see https://github.com/mndrake/ExcelDnaDoc.</description>
    <releaseNotes>bug fix - if referenced assemblies referred to additional assemblies documentation failed</releaseNotes>
    <copyright>Copyright 2013</copyright>    
    <tags>Excel-DNA Excel</tags>
    <dependencies><group><dependency id="Excel-DNA" version="0.30.3" /></group></dependencies>
  </metadata>
  <files>
    <file src="..\bin\ExcelDna.Documentation.dll" target="lib/net40" />
    <file src="..\bin\ExcelDna.Documentation.dll" target="tools" />
    <file src="..\bin\ExcelDna.Documentation.dll" target="content/HelpContent/bin" />    
    <file src="..\bin\ExcelDna.Integration.dll" target="tools" />
    <file src="..\bin\ExcelDnaDoc.exe" target="tools" />
    <file src="..\bin\ExcelDnaDoc.exe.config" target="tools" />
    <file src="..\bin\RazorEngine.dll" target="tools" />
    <file src="..\bin\System.Web.Razor.dll" target="tools" />
    <file src="..\bin\HelpContent\*.*" target="content/HelpContent" />
    <file src="..\bin\HelpContent\*.*" target="tools/HelpContent" />
    <file src="..\bin\web.config" target="tools" />
    <file src="..\nuget\install.ps1" target="tools" />
    <file src="..\nuget\uninstall.ps1" target="tools" />    
    <file src="..\nuget\ExcelDnaDoc-Template.dna" target="content" />
    <file src="..\README.md" target="" />
  </files>
</package>
